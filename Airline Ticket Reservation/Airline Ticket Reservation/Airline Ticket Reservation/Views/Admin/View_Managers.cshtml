@model List<Airline_Ticket_Reservation.Models.ManagerRegistrationViewModel>
@{
    ViewBag.Title = "Manager Details";
}
<h2>@ViewBag.Title.</h2>
<table class="grid" border="1">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>EmailID</th>
        <th>Mobile Number</th>        
        <th>Gender</th>
        <th>Country</th>
        <th>State</th>
        <th>Address</th>
    </tr>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
        <tr>
            <td>
                @item.txtfname
            </td>
            <td>
                @item.txlname
            </td>
            
            <td>
                @item.txtEmail
            </td>
            <td>
                @item.txtMob
            </td>
            <td>
                @item.ddlgender
            </td>
            <td>
                @item.ddlcountry
            </td>
            <td>
                @item.ddlstate
            </td>
            <td>
                @item.txtaddress
            </td>
            
        </tr>
        }
    }

</table>
